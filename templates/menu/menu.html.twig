{% extends 'base.html.twig' %}

{% block body %}

  <h1>Nos Menus</h1>

  <button class="btn btn-outline-primary" onclick="rtn()">Retour</button>

  <br></br>

    {% for menu in menus %}
      <button type="button" class="btn btn-primary btn-lg"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;"><a class="nav-link" href="/panier/add/{{ menu.id }}">{{ menu.title }}</a></font></font></button>
    {% endfor %}


 <script>
   function rtn() {
     window.history.back();
   }
 </script>
{% endblock %}    